{
  "tasks": {
    "start": "deno run -A --watch src/main.ts"
  },
  "imports": {
    "preact": "npm:preact@10",
    "preact-render-to-string": "npm:preact-render-to-string@6",
    "@std/http": "jsr:@std/http@1",
    "@std/ulid": "jsr:@std/ulid@1"
  },
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "npm:preact"
  },
  "lint": {
    "rules": {
      "exclude": ["jsx-button-has-type"]
    }
  },
  "unstable": ["kv", "cron"]
}
